{
    "manifest_version": 2,
    "name": "Scaper",
    "version": "0.1",
    "browser_action": {
    },
    "content_security_policy": "script-src 'self' 'sha256-PEQCWT3cQigJbDLhdvKflIkGYp1+gG7JC+SDAPwAu/k=' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
